@article{fernandes_origins_2007,
	title = {Origins of prehistoric flints: The neocortex memory revealed by scanning electron microscopy},
	volume = {6},
	doi = {10.1016/j.crpv.2007.09.015},
	shorttitle = {Origins of prehistoric flints: The neocortex memory revealed by scanning electron microscopy},
	abstract = {Flint has been widely used during prehistoric times and the question of determining the origin of specific pieces is recurrent in archaeological discussions. We have run a scanning electron microscope examination of the surfaces of both natural flints from geological deposits and flaked artefacts excavated from prehistoric sites in the Massif Central (France). These samples exhibit mineralogical textures resulting from genomorphic and phenomorphic processes, dispersed alteration in the form of erosion as well as the dissolution and migration of more or less mobile elements. Rocks used to manufacture artefacts at Sainte-Anne 1 (Haute-Loire, France) and Payre (Ard`eche, France) show various kinds of alteration related to the range of phases of transport and deposition to which they were subjected, beginning with their first exposure in a geological context and following through to the post-depositional phase, (i.e., following abandonment as artefacts in an archaeological site). Later phenomena do not however completely obliterate stigma imprinted prior to their prehistoric collection, because the main late-phase change that occurs on flat surfaces prior to their collection is preserved in the hollows of remnant natural surfaces on archaeological tools. With the scanning electron microscope, five distinct morphologies representative of different environments have been characterised by establishing associations between the different forms and by plotting their regional distribution.},
	pages = {557--568},
	journaltitle = {Compets Rendus Palevol},
	author = {Fernandes, Paul and Le Bourdonnec, François-Xavier and Raynal, Jean-Paul and Poupeau, Gérard and Piboule, Michel and Moncel, Marie-Hélène},
	date = {2007},
	keywords = {Ard`eche, Flint, France, Gathering outcrop, Haute-Loire, Middle Palaeolithic, Neocortex, {SEM}},
	file = {Fernandes et al. - 2007 - Origins of prehistoric flints The neocortex memor.pdf:C\:\\Users\\Guillermo\\Zotero\\storage\\UTCS76FC\\Fernandes et al. - 2007 - Origins of prehistoric flints The neocortex memor.pdf:application/pdf},
}

