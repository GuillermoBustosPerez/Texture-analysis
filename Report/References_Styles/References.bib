@incollection{schick_experimentally-derived_1987,
	title = {Experimentally-derived criteria for assessing hydrologic disturbance of archaeological sites},
	isbn = {0 86054 455 9},
	series = {{BAR} International Series},
	abstract = {The majority of Lower Palaeolithic sites which form our data base for the early phases of human cultural evolution have been recovered from within sedimentary deposits formed along ancient river systems or in the vicinity of inland lakes (e.g., Hay 1976; Isaac 1972; Leakey 1971; Toth and Schick 1986; Wymer 1976). A major question, then, to the archaeologist interested in maximizing the reliability of behavioral interpretations of the early archaeological record or from later periods of prehistory, concerns how these archaeological deposits have been influenced and modified by the sedimentary processes that contributed to their formation. In particular, how might their composition have been changed, affecting interpretations and comparisons between sites and artifact assemblages in different places and times, and how might the spatial arrangement of their materials have been altered, affecting spatial analyses and detection of activity areas and toolkits?},
